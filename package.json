{
  "name": "11ty-portfolio",
  "version": "1.1.0",
  "description": "My personal website built with the static site generator Eleventy",
  "main": ".eleventy.js",
  "scripts": {
    "watch:sass": "sass --watch src/_includes/sass:src/_includes/css",
    "build:sass": "sass src/_includes/sass:src/_includes/css",
    "watch:eleventy": "eleventy --serve",
    "build:eleventy": "eleventy",
    "dev": "npm-run-all build:sass --parallel watch:*",
    "build": "npm-run-all build:*",
    "debug": "DEBUG=* eleventy",
    "test": "jest"
  },
  "keywords": [
    "static site",
    "Eleventy",
    "11ty"
  ],
  "author": {
    "name": "Tanner Dolby",
    "email": "dolb.tanner@gmail.com",
    "url": "https://github.com/tannerdolby"
  },
  "license": "MIT",
  "dependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-cache-assets": "^2.2.1",
    "@types/jest": "^26.0.23",
    "capture-website": "^1.4.0",
    "cheerio": "^1.0.0-rc.9",
    "clean-css": "^4.2.3",
    "eleventy-plugin-metagen": "^1.3.1",
    "eleventy-plugin-sharp-respimg": "^1.2.8",
    "eleventy-plugin-social-img": "^1.2.6",
    "markdown-it": "^12.0.6",
    "npm-run-all": "^4.1.5",
    "nunjucks-date-filter": "^0.1.1",
    "sass": "^1.34.0",
    "slugify": "^1.5.3",
    "speedlify-score": "^2.0.2",
    "terser": "^5.7.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tannerdolby/tannerdolby.com.git"
  },
  "bugs": {
    "url": "https://github.com/tannerdolby/tannerdolby.com/issues"
  },
  "homepage": "https://github.com/tannerdolby/tannerdolby.com/#readme",
  "devDependencies": {
    "@11ty/eleventy-plugin-rss": "^1.1.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.0",
    "babel-preset-env": "^1.7.0",
    "grunt": "^1.4.0",
    "jest": "^26.6.3",
    "node-fetch": "^2.6.1"
  }
}
